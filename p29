static void p29(){
        int n=5;
        int mid=n/2;
    for (int i=0;i<n;i++){
    for (int j=0;j<n;j++){   
        if(Math.abs(i-mid)+Math.abs(j-mid)==mid) 
        System.out.print((char)('A'+Math.abs(i-mid)));
        else
        System.out.print(" ");}
    System.out.println();
    }
    }
